{
  "name": "bandcamp-shortcuts-extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Simple Bandcamp keyboard shortcut extension",
  "content_scripts": [
    {
      "matches": [
        "*://*.bandcamp.com/*"
      ],
      "js": [
      	"jquery-1.11.2.min.js",
        "inject.js"
      ]
    }
  ]
}